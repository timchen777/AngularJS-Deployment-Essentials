{
  "name" : "krakn",
  "description" : "Real-time chat application built with AngularJS, Ionic and Firebase as an example for the book AngularJS Deployment Essentials.",
  "version" : "1.0",
  "manifest_version" : 2,
  "app" : {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "content_security_policy" : "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",
  "icons" : {
    "16" : "icon/icon_16.png",
    "48" : "icon/icon_48.png",
    "128" : "icon/icon_128.png"
  }
}